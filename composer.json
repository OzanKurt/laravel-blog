{
    "name": "ozankurt/laravel-blog",
    "description": "Ready to use blog module for laravel.",
    "keywords": ["laravel", "blog", "module"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ozan Kurt",
            "email": "me@ozankurt.com",
            "homepage": "https://ozankurt.com"
        }
    ],
    "require": {
        "spatie/laravel-sluggable": "^3.7",
        "spatie/laravel-medialibrary": "^11.11"
    },
    "require-dev": {
        "orchestra/testbench": "^9.8"
    },
    "suggest": {
        "spatie/laravel-tags": "To tag posts."
    },
    "autoload": {
        "psr-4": {
            "OzanKurt\\Blog\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "OzanKurt\\Blog\\BlogServiceProvider"
            ]
        }
    }
}
